.mask-wrap {
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.6);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask-content-wrap{
  width:86%;
  height:846rpx;
  position:absolute;
  top:50%;
  left:0;
  transform:translateY(-50%);
  margin-left:7%;
  z-index:10000;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.mast-top {
  width: 85%;
  height: 100%;
  border-radius: 14rpx;
  background: #fff;
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
  line-height:42px;
}
.bottom{
  width: 100%;
  height: 80rpx;
  margin-top: 53rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.close {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mast-top {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.status-img {
  width: 100%;
  height: 280rpx;
  margin-bottom: 30rpx;
  border-top-right-radius: 14rpx;
  border-top-left-radius: 17rpx;
}
.title {
  font-size:64rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  margin-top:30rpx;
  position:absolute;
  color:#fff;

}
.dis-msg {
  width: 90%;
  color:rgba(102,102,102,1);
  margin-top: 20rpx;
  line-height: 50rpx;
}
.share-btn{
  width: 300rpx;
  line-height: 70rpx;
  background:rgba(247,147,30,1);
  border-radius:35rpx;
  margin-top: 84rpx;
  font-size:30rpx;
  font-family:MicrosoftYaHei;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.close-img {
  width: 50%;
  height: 50%;
  opacity: .5;
}