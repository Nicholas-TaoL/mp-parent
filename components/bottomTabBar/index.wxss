.bar-ctn{
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 98rpx;
  width: 100%;
  border-top: 1px solid #D5D7D8;
  position: fixed;
  bottom: 0;
  background: darkcyan;
}
.tab-img{
  width: 44rpx;
  height: 43rpx;
}
.bar{
  flex: 1;
  display: flex;
  padding-top: 21rpx;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  
}
.tab-title{
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
}
.color {
  color: #333;
}
.select-color {
  color: #F7931E;
}
.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 120rpx;
    border-top: 1rpx solid gray; 
}

.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    height: 100%;
}

.tabbar_icon{
    width: 61rpx;
    height: 61rpx;
}



