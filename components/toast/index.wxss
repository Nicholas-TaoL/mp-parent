.mask-wrap {
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.6);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask-content-wrap{
  width:70%;
  min-height: 379rpx;
  position:absolute;
  top:50%;
  left:0;
  transform:translateY(-50%);
  margin-left:15%;
  z-index:10000;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.mast-top {
  width: 80%;
  height: 80%;
  border-radius: 14rpx;
  background: #fff;
}
.bottom{
  width: 100%;
  height: 80rpx;
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.close {
  height: 100%;
  width: 80rpx;
  border-radius: 50%;
  background-color: #fff;
}
.mast-top {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 40rpx ;
}
.status-img {
  width: 157rpx;
  height: 105rpx;
}
.title {
  font-size:36rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(53,53,54,1);
  margin-top: 30rpx;
}
.dis-msg {
  width: 80%;
  font-size:30rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
  text-align:center;
  line-height: 50rpx;
}