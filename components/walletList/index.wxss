.wallert-list-ctn {
  display: flex;
  flex-direction: column;
}
.list-item-wrap {
  height: 98rpx;
  border-bottom: 1rpx solid rgba(220,220,220,1);
  box-sizing: border-box;
  padding: 0 27rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.item-left {
  display: flex;
  align-items: center;
}
.icon {
  width: 34rpx;
  height: 34rpx;
}
.label {
  font-size:26rpx;
  font-family:Adobe Heiti Std R;
  font-weight:normal;
  color:rgba(102,102,102,1);
  box-sizing: border-box;
  padding-left: 14rpx;
}
.next {
  width: 9rpx;
  height: 18rpx;
}