.payed-ctn {
  height: 100%;
  width: 100%;
  background:rgba(246,246,246,1);
  position: relative;
}
.top-wrap {
  width: 100%;
  height: 220rpx;
  display: flex;
  box-sizing: border-box;
  padding-left: 24rpx;
}
.top-left-img-wrap {
  width:200rpx;
  height:100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top-left-img {
  width:150rpx;
  height:150rpx;

}
.top-right-wrap {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 33rpx 0;
  font-family:Adobe Heiti Std R;
  font-weight:normal;
  padding: 0 32rpx;
}
.top-r-title-wrap , .top-r-good-wrap{
  display: flex;
  align-items: center;
}
.t-title {
  font-size:30rpx;
  color:rgba(51,51,51,1);
}
.is-tuanzhang {
  width:50rpx;
  height:30rpx;
  border:1rpx solid rgba(247,147,30,1);
  border-radius:4rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(247,147,30,1);
  margin-left: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.t-des {
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin: 12rpx 0;
  height: 66rpx;
  width: 550rpx;

  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.t-r-img-wrap {
  width: 20rpx;
  height: 20rpx;
}
.t-r-good {
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(153,153,153,1);
}

.middle-wrap {
  width: 100%;
  height: 340rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 44rpx 0;
  position: relative;
}
.end-time {
  height:23rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.member-wrap {
  height: 110rpx;
  width: 100%;
  position: relative;
  margin-top: 38rpx;
  margin-bottom: 54rpx;
  display: flex;
  justify-content: center;
}
.img-wrap-tuan{
  height: 100%;
  width: 86rpx;
  position: relative;
  margin-right: 20rpx;
}
.is-tuanzhang {
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translateX(-50%);
  width:58rpx;
  line-height:28rpx;
  background:rgba(254,92,0,1);
  border-radius:6rpx;
  text-align: center;
  font-size:20rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin: 0 auto;
}
.tx-img {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  background-color: #EBEBEB;
  margin-right: 28rpx;
}
.end-nums {
  height:30rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.ptzhing {
  width:100rpx;
  height:45rpx;
  background:rgba(254,92,0,1);
  border-radius:0px 21rpx 21rpx 0px;
  position: absolute;
  left: 0;
  top: 22rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom-wrap {
  position: absolute;
  height: 100rpx;
  width: 100%;
  background:linear-gradient(90deg,rgba(254,190,94,1),rgba(255,137,3,1));
  left: 0;
  bottom: 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.red-nums {
  color: #FE5C00;
}
.end-red-time {
  color: #FF0000;
}
.bottom-share-btn{
  position: absolute;
  left: 0;
  bottom: 0rpx;
  z-index: 33;
  height: 100rpx;
  width: 100%;
  opacity: 0;
}