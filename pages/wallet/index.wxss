/**index.wxss**/
.wallet-ctn {
  height: 100%;
}
.top-ctn{
  height: 282rpx;
  width: 100%;
  display: flex;
  align-items: center;
  background-image: url(data:image/png;base64,);

}
.top-content {
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
}
.price-top {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-bottom: 34rpx;
}
.gold-coin {
  width: 30rpx;
  height: 30rpx;
}
.gold-label {
  box-sizing: border-box;
  padding-left: 8rpx;
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.gold-num {
  font-size:60rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height:40rpx;
}
.withdrawal-wrap {
  height: 85rpx;
  margin-top: 177rpx;
  display: flex;
  justify-content: center;
}
.withdrawal-btn {
  width: 710rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
  font-size:30rpx;
  font-family:Adobe Heiti Std R;
  font-weight:normal;
  color:rgba(255,255,255,1);
}