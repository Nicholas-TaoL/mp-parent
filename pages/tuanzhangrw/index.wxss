.rw-ctn {
  height: 100%;
  width: 100%;
  font-family:PingFang-SC-Medium;
  font-weight:500;
}
.banner {
  width: 100%;
  height:286rpx;
  background:rgba(247,147,30,1);
}
.center-wrap {
  width: 100%;
  height: 135rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.center {
  width: 320rpx;
  height: 75rpx;
}

.rw-item {
  height: 180rpx;
  width: 100%;
  display: flex;
  box-sizing: border-box;
  padding: 0 24rpx;
}
.item-right {
  width: 150rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.item-left{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.share-btn {
  width:150rpx;
  height:70rpx;
  background:rgba(247,147,30,1);
  border-radius:30rpx;
  font-size:28rpx;
  color:rgba(255,255,255,1);
  display: flex;
  justify-content: center;
  align-items: center;
}
.v-line {
  width:6rpx;
  height:30rpx;
  background:rgba(247,147,30,1);
  margin-right: 26rpx;
}
.jlmsg , .title-wrap{
 width: 100%;
 height: 40rpx;
 display: flex;
 align-items: center;
}
.jlmsg  {
  margin-top: 27rpx;
}
.jb-img {
  width: 44rpx;
  height: 30rpx;
  margin-right: 26rpx;
}
.label {
  font-size:24rpx;
  color:rgba(247,147,30,1);
}
.top-title {
  font-size:30rpx;
  color:rgba(51,51,51,1);
}

