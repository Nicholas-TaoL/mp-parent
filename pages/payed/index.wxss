/**index.wxss**/
page{
  height: 100%;
  background-color: #fff;
}
.payed-ctn {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
}
.pay-bottom-ctn{
  height: 106rpx;
  width: 100%;
  box-shadow:0px -1px 18px rgba(194,195,197,.2);
  display: flex;
}
.pay-bottom-right{
  height: 100%;
  width: 193rpx;
  background-color: rgba(254,92,0,1);
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.pay-bottom-left{
  flex: 1;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 49rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.pay-type-ctn {
  height: 79rpx;
  border-top:1rpx solid rgba(240,240,240,1);
  border-bottom:1rpx solid rgba(240,240,240,1);
  margin-top: 19rpx;
  display: flex;
  padding: 0 54rpx;
  justify-content: space-between;
  align-items: center;
}
.pay-title{
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
}