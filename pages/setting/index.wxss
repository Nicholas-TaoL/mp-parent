.setting-ctn {
  height: 100%;
  display: flex;
  flex-direction: column;
  background:rgba(246,246,246,1);
}
.list-wrap {
  box-shadow: 0px 1px 8px rgba(194,195,197,.2);
}
.out-btn{
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background:rgba(255,255,255,1);
  border: none;
  border-top: 1px solid rgba(213, 215, 216, 1);
  position: fixed;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,1,1,1);
  bottom: 0;
  left: 0;
  border-radius: 0;
  
}